# Create a list of numbers
numbers = [1, 3, 5, 7, 9]

# Print the list
print("Original list:", numbers)

# Access elements by index
first_element = numbers[0]
print("The first element is:", first_element)


# Slice the list to get a subset
subset = numbers[2:4]
print("Subset of the list:", subset)

# Modify an element in the list
numbers[3] = 10
print("Modified list:", numbers)



# Append an element to the end of the list
numbers.append(11)
print("List after appending 11:", numbers)

# Remove an element by value
numbers.remove(5)
print("List after removing 5:", numbers)


# Find the index of an element
index_of_3 = numbers.index(3)
print("Index of 3:", index_of_3)

# Check if an element is in the list
contains_4 = 4 in numbers
print("Does the list contain 4?", contains_4)


# Sort the list
numbers.sort()
print("Sorted list:", numbers)

# Reverse the list
numbers.reverse()
print("Reversed list:", numbers)

 
# Create a list of strings
fruits = ["apple", "banana", "cherry", "date"]

print(fruits[0])
